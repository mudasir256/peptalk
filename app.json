{
  "expo": {
    "name": "peptalk",
    "slug": "peptalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Rubik-Black.ttf",
            "./src/assets/fonts/Rubik-BlackItalic.ttf",
            "./src/assets/fonts/Rubik-Bold.ttf",
            "./src/assets/fonts/Rubik-BoldItalic.ttf",
            "./src/assets/fonts/Rubik-ExtraBold.ttf",
            "./src/assets/fonts/Rubik-ExtraBoldItalic.ttf",
            "./src/assets/fonts/Rubik-Italic.ttf",
            "./src/assets/fonts/Rubik-Light.ttf",
            "./src/assets/fonts/Rubik-LightItalic.ttf",
            "./src/assets/fonts/Rubik-Medium.ttf",
            "./src/assets/fonts/Rubik-MediumItalic.ttf",
            "./src/assets/fonts/Rubik-Regular.ttf",
            "./src/assets/fonts/Rubik-SemiBold.ttf",
            "./src/assets/fonts/Rubik-SemiBoldItalic.ttf"
          ]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mombrain.peptalk",
      "usesAppleSignIn": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mombrain.peptalk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}